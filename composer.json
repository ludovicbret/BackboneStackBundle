{
    "name"       : "lb/backbone-stack",
    "type"       : "symfony-bundle",
    "description": "Backbone stack for Symfony2",
    "keywords"   : ["backbone", "form", "utils", "js"],
    "license"    : "MIT",
    "version"    : "master",
    "authors"    : [
        {
            "name": "Ludovic Bret",
            "email": "bretludovic@googlemail.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "Lb\\BackboneStackBundle": ""
        }
    },
    "target-dir": "Lb/BackboneStackBundle",
    "require"   : {
        "symfony/symfony"          : "2.1.*",
        "mopa/composer-bridge"     : "dev-master",
        "hearsay/require-js-bundle": "dev-master"
    },
    "suggest":  {
        "twitter/bootstrap"                : "dev-master",
        "documentcloud/underscore"         : "dev-master",
        "documentcloud/backbone"           : "dev-master",
        "douglascrockford/JSON-js"         : "dev-master",
        "friendsofsymfony/jsrouting-bundle": "dev-master",
        "powmedia/backbone-forms"          : "dev-master"
    },
    "repositories": [
        {
            "type"   : "package",
            "package": {
                "version": "dev-master",
                "name"   : "documentcloud/backbone",
                "source" : {
                    "url"      : "https://github.com/documentcloud/backbone.git",
                    "type"     : "git",
                    "reference": "master"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "version": "dev-master",
                "name"   : "documentcloud/underscore",
                "source" : {
                    "url"      : "https://github.com/documentcloud/underscore.git",
                    "type"     : "git",
                    "reference": "master"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "version": "dev-master",
                "name"   : "douglascrockford/JSON-js",
                "source" : {
                    "url"      : "https://github.com/douglascrockford/JSON-js.git",
                    "type"     : "git",
                    "reference": "master"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "version": "dev-master",
                "name"   : "powmedia/backbone-forms",
                "source" : {
                    "url"      : "https://github.com/powmedia/backbone-forms.git",
                    "type"     : "git",
                    "reference": "master"
                }
            }
        },
        {
            "type"   : "package",
            "package": {
                "version": "dev-master",
                "name"   : "friendsofsymfony/jsrouting-bundle",
                "source" : {
                    "url"      : "https://github.com/FriendsOfSymfony/FOSJsRoutingBundle.git",
                    "type"     : "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "version": "dev-master",
                "name": "twitter/bootstrap",
                "source": {
                    "url": "https://github.com/twitter/bootstrap.git",
                    "type": "git",
                    "reference": "master"
                },
                "dist": {
                    "url": "https://github.com/twitter/bootstrap/zipball/master",
                    "type": "zip"
                 }
            }
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ],
        "post-update-cmd": [
            "Mopa\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::postInstallSymlinkTwitterBootstrap"
        ]
    },
    "minimum-stability": "dev"    
}
